service_name: <%= service_name %>

model_name: <%= model %>

<% if attrs && !attrs.empty? %>
attrs: !omap
  <% attrs.each_pair do |k, v| %>
  - <%= k %>: <%= v %>
  <% end %>
<% end %>

<% if resources && !resources.empty? %>
resources:
  <% resources.each do |r| %>
  - name: <%= r.name %>
    method: <%= r.method %>
    path: <%= r.path %>
    params: <%= r.params %>
  <% end %>
<% end %>
